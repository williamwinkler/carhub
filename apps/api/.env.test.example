# Example test environment configuration
# Copy this file to .env.test and modify as needed for your test environment

# Test Database Configuration
# If these are not set, the main database configuration will be used
TEST_POSTGRES_HOST=localhost
TEST_POSTGRES_PORT=5433
TEST_POSTGRES_DATABASE=demo_test_db
TEST_POSTGRES_USERNAME=test_user
TEST_POSTGRES_PASSWORD=test_password

# Optional: Drop schema on each test run (use with caution!)
TEST_DROP_SCHEMA=true

# Set to test environment
NODE_ENV=test